<UserControl x:Class="GlobalMessages.ConsoleContainer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:usercontrols="clr-namespace:Controls;assembly=Controls"
             xmlns:ViewModels="clr-namespace:Global.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    
    <UserControl.DataContext>
        <ViewModels:ConsoleContainerViewModel />
    </UserControl.DataContext>
    
    <usercontrols:Console ConsoleLinesSource="{Binding ConsoleLines}" Grid.Row="1" />
</UserControl>
